{
    // Date range
    "Date.Range": {
        "start.year": 1981,
        "start.mon": 1,
        "start.day": 1,
        "end.year": 2015,
        "end.mon": 12,
        "end.day": 31
    },
    // Minimum % of non-missing of the stations series to be accepted to the aggregated data
    "min.perc": 1,
    // format of input stations data, 
    // "Multiple": one file per station
    // "Single": all stations are grouped into a single file
    "data.type": "Multiple",

    // Input and Output files/directory
    "IO.files": {
        // Station data sample file for "Multiple Files" type
        "STN.sample.file": "",
        // Full path to the directory of station data  for "Multiple Files" type
        "STN.dir": "",
        // Station data for "Single File" type
        "STN.single.file": "",
        // station coordinates file
        "STN.coords.file": "",
        // file path to save formated data
        "File2Save": ""
    },
    // Multiple Files: each station in a separate file
    "Multiple.File": {
        // Series format, 1: one variable, 3: 3 variables (Rain, Tmax, Tmin)
        "file.format": 1,
        // Date format, 1: grouped (YYYYMMDD), 3: separated (YYYY MM DD)
        "date.format": 1,
        // include elevation data to the output CDT format (if exist): false or true
        "include.elev": false
    },
    // Single Files: all station in a single file
    "Single.File": {
        // Coordinates included in the data file: true if else false 
        "coords.included": true,
        // number of column containing the data 
        // Daily : 1 or 31
        // Dekadal: 1, 3 or 36
        // Monthly: 1 or 12
        "nb.column": 31,
        // Index of the column contains the station ID
        "col.stn.id": 1,
        // Index of the column contains the longitude if included
        "col.stn.lon": 2,
        // Index of the column contains the latitude if included
        "col.stn.lat": 3,
        // Index of the column contains the elevation if included
        "col.stn.elv": 4,
        // Index of the column contains the years
        "col.year": 5,
        // Index of the column contains the months
        "col.month": 6,
        // Index of the column contains the days or dekad
        "col.day.dek": 7,
        // Index of the column from which the data start
        "col.start.data": 8,
        // include elevation data to the output CDT format (if exist): false or true
        "include.elev": false
    }
}
