<?xml version="1.0" encoding="UTF-8"?>
<dialog_box>
    <title>
        <text lang="en"> Interpolation parameters </text>
    </title>
    <widget>
        <combobox id="1">
            <option id="1">
                <text lang="en"> Inverse Distance Weighted </text>
            </option>
            <option id="2">
                <text lang="en"> Ordinary Kriging </text>
            </option>
            <option id="3">
                <text lang="en"> Universal Kriging </text>
            </option>
            <option id="4">
                <text lang="en"> Modified Shepard interpolation </text>
            </option>
            <option id="5">
                <text lang="en"> Spheremap interpolation method </text>
            </option>
            <option id="6">
                <text lang="en"> Nearest Neighbor </text>
            </option>
            <option id="7">
                <text lang="en"> Nearest Neighbor with elevation - 3D </text>
            </option>
        </combobox>
        <checkbutton id="1">
            <text lang="en"> Use block mean values </text>
        </checkbutton>
        <checkbutton id="2">
            <text lang="en"> Elevation </text>
        </checkbutton>
        <checkbutton id="3">
            <text lang="en"> Slope </text>
        </checkbutton>
        <checkbutton id="4">
            <text lang="en"> Aspect </text>
        </checkbutton>
        <checkbutton id="5">
            <text lang="en"> Longitude </text>
        </checkbutton>
        <checkbutton id="6">
            <text lang="en"> Latitude </text>
        </checkbutton>
        <label id="1">
            <text lang="en"> Variogram Model </text>
        </label>
        <label id="2">
            <text lang="en"> Minimum number of points </text>
        </label>
        <label id="3">
            <text lang="en"> Radius of Influence </text>
        </label>
        <label id="4">
            <text lang="en"> Horizontal Radius of Influence </text>
        </label>
        <label id="5">
            <text lang="en"> Elevation data (netCDF) </text>
        </label>
        <label id="6">
            <text lang="en"> Interpolation method </text>
        </label>
    </widget>
    <help>
        <content id="1">
            <status>
                <text lang="en"> Minimum number of neighbors to be used to interpolate data </text>
            </status>
